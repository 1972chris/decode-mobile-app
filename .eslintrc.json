{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "jest": true
  },
  "globals": {
    "browser": true,
    "expect": true,
    "API": true,
    "expoLink": true,
    "$": true,
    "neptuneUrl": true,
    "atlantisUrl": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-console": 0
  }
}
