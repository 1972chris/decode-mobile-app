diff --git a/lib/genders.js b/lib/genders.js
index e69de29..2395825 100644
--- a/lib/genders.js
+++ b/lib/genders.js
@@ -0,0 +1,13 @@
+
+const genders = {
+  F: 'female',
+  M: 'male',
+  O: 'other',
+};
+
+export function sortedGendersList() {
+  const alphabeticalComparator = (a, b) => (a[1] > b[1] ? 1 : -1);
+
+  return Object.entries(genders)
+    .sort(alphabeticalComparator);
+}
